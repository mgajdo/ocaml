(library
 (name crunch)
 (kind normal)
 (synopsis "Compile static files into a static filesystem")
 (archives (byte crunch.cma) (native crunch.cmxa))
 (plugins (byte crunch.cma) (native crunch.cmxs))
 (native_archives crunch.a)
 (requires unix ptime)
 (modes byte native)
 (modules
